# Google Cloud Configuration (Required)
GOOGLE_CLOUD_PROJECT=your-project-id
GOOGLE_CLOUD_LOCATION=us-central1
GOOGLE_PROJECT_NUMBER=your-project-number

# Google OAuth Configuration (Required for Drive access and duplicate detection)
GOOGLE_OAUTH_CLIENT_ID=your-oauth-client-id.apps.googleusercontent.com
GOOGLE_OAUTH_CLIENT_SECRET=your-oauth-client-secret

# Google API Keys (Required)
GOOGLE_API_KEY=your-google-api-key
GOOGLE_GENAI_API_KEY=your-genai-api-key
GOOGLE_GENAI_MODEL=gemini-2.0-flash-001

# AgentSpace Configuration (Required)
AS_APP=your-agentspace-app-id
AUTHORIZATION_NAME=plan_on_page_auth_001

# Environment Flags
LOCAL_DEV=1
ADK_DEV_MODE=false
ADK_LOCAL_RUN=false
AGENTSPACE_DEPLOYMENT=false

# Deployment Configuration
STAGING_BUCKET=gs://your-staging-bucket

# RAG Configuration (Optional - for enhanced planning knowledge)
# Uncomment these when you have set up a Vertex AI Search corpus for planning best practices
# RAG_CORPUS=projects/your-project/locations/us-central1/ragCorpora/plan_knowledge_corpus_id
# VERTEX_SEARCH_ENGINE_ID=your-search-engine-id
# PLAN_KNOWLEDGE_CORPUS_NAME=plan-on-page-knowledge-base
# PLAN_KNOWLEDGE_SEARCH_LOCATION=us-central1
